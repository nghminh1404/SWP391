<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SettingController</servlet-name>
        <servlet-class>Controller.SettingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileController</servlet-name>
        <servlet-class>Controller.ProfileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfileController</servlet-name>
        <servlet-class>Controller.EditProfileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DemoDashboardController</servlet-name>
        <servlet-class>Controller.DemoDashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePasswordController</servlet-name>
        <servlet-class>Controller.ChangePasswordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeAvatar</servlet-name>
        <servlet-class>Controller.AvatarController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WebContactInfoController</servlet-name>
        <servlet-class>Controller.WebContactInfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassSettingController</servlet-name>
        <servlet-class>Controller.ClassSettingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassSettingDelete</servlet-name>
        <servlet-class>Controller.ClassSettingDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MilestoneController</servlet-name>
        <servlet-class>Controller.MilestoneController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleController</servlet-name>
        <servlet-class>Controller.ScheduleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleDetailController</servlet-name>
        <servlet-class>Controller.ScheduleDetailController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileController</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileController</servlet-name>
        <url-pattern>/editprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SettingController</servlet-name>
        <url-pattern>/setting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DemoDashboardController</servlet-name>
        <url-pattern>/demodashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePasswordController</servlet-name>
        <url-pattern>/changepassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeAvatar</servlet-name>
        <url-pattern>/changeavatar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WebContactInfoController</servlet-name>
        <url-pattern>/web_contact_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassSettingController</servlet-name>
        <url-pattern>/classsetting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassSettingDelete</servlet-name>
        <url-pattern>/classsettingdelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MilestoneController</servlet-name>
        <url-pattern>/milestone</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleController</servlet-name>
        <url-pattern>/schedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleDetailController</servlet-name>
        <url-pattern>/scheduledetail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
